{
  "name": "productized-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prelocal": "pnpm db:generate",
    "local": "concurrently -n nextjs,proxy,docker-mysql,prisma-studio -c cyan,magenta,yellow,green \"pnpm dev\" \"pnpm lt\" \"pnpm db:dev\" \"pnpm db:studio\"",
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "db:dev": "docker compose up",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx scripts/seed.ts",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "lt": "lt --port 3000 --subdomain productized-store-00fff",
    "ip": "tsx scripts/ip.ts",
    "preview": "pnpm build && pnpm start",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": "prettier --write"
  },
  "dependencies": {
